<h1>New user</h1>

<%= form_for(@administrator) do |f| %>
                <div class="field">
                    <%= f.label :nombre %> <br />
                    <%= f.text_field :nombre %>
                </div>
                <div class="field">
                    <%= f.label :email %> <br />
                    <%= f.text_field :email %>
                </div>
                <div class="field">
                    <%= f.label :admin %> <br />
                    <%= f.text_field :admin %>
                </div>
                <div class="field">
                    <%= f.label :password %> <br />
                    <%= f.password_field :password %>
                </div>
                <div class="field">
                    <%= f.label :password_confirmation, "Confirmation" %> <br />
                    <%= f.password_field :password_confirmation %>
                </div>
                <div class="actions">
                        <%= f.submit "Create"%> <br />
                </div>
            <%end%>
<%= link_to 'Back', users_path %>
