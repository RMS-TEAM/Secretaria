DROP PROCEDURE IF EXISTS proc_res5_9_1;

DELIMITER //

CREATE PROCEDURE proc_res5_9_1()
BEGIN
	DELETE FROM `res5_9_1`;
	INSERT INTO `res5_9_1` (`dane`, `aprobacion5`, `aprobacion9`, `nombre`, `Progresolen5`, `Progresolen9`, `Progresomat5`, `Progresomat9`, `a単o_indicador`) 
	SELECT l.`dane`, `aprobacion5`, `aprobacion9`, `nombre`, `Progresolen5`, `Progresolen9`, `Progresomat5`, `Progresomat9`, l.`a単o_indicador`
	FROM  `taprob` l, `permedianocr` r WHERE l.`dane` =  r.`dane`;
	
	INSERT INTO `res5_9_1` (dane, aprobacion5, aprobacion9)
	SELECT dane, aprobacion5, aprobacion9 FROM `taprob`
	WHERE NOT EXISTS (SELECT * FROM `permedianocr` where `permedianocr`.dane = `taprob`.dane);

	INSERT INTO `res5_9_1` (`dane`, `nombre`, `Progresolen5`, `Progresolen9`, `Progresomat5`, `Progresomat9`, `a単o_indicador`)
	SELECT `dane`, `nombre`, `Progresolen5`, `Progresolen9`, `Progresomat5`, `Progresomat9`, `a単o_indicador` FROM `permedianocr`
	WHERE NOT EXISTS (SELECT * FROM `taprob` where `permedianocr`.dane = `taprob`.dane);

END //

DELIMITER ;

