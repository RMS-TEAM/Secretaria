DROP PROCEDURE IF EXISTS proc_res11i;

DELIMITER //

CREATE PROCEDURE proc_res11i()
BEGIN

	DELETE FROM `res11i`;
	INSERT INTO `res11i` (cole_inst_nombre, dane, tema_matematica, tema_lenguaje, prmat_bajo_07, prmat_medio_07, prmat_alto_07, 
	 prlen_bajo_07, prlen_medio_07, prlen_alto_07, prmat_bajo_07_p, prmat_medio_07_p, prmat_alto_07_p, prlen_bajo_07_p, 
	 prlen_medio_07_p, prlen_alto_07_p, tema_lenguaje_d, tema_matematica_d, prmat_bajo_10, prmat_medio_10, prmat_alto_10, 
	 prlen_bajo_10, prlen_medio_10, prlen_alto_10, prmat_bajo_10_p, prmat_medio_10_p, prmat_alto_10_p, prlen_bajo_10_p, 
	 prlen_medio_10_p, prlen_alto_10_p, permed50len, permed33len, permed50mat, permedmat33, aprobacion5, aprobacion9, aprobacion11a, 
	 aprobacion11t)
	SELECT res11.cole_inst_nombre, res11.dane, tema_matematica, tema_lenguaje, prmat_bajo_07, prmat_medio_07, prmat_alto_07, prlen_bajo_07, prlen_medio_07, prlen_alto_07, prmat_bajo_07_p, prmat_medio_07_p, prmat_alto_07_p, prlen_bajo_07_p, prlen_medio_07_p, prlen_alto_07_p, tema_lenguaje_d, tema_matematica_d, prmat_bajo_10, prmat_medio_10, prmat_alto_10, prlen_bajo_10, prlen_medio_10, prlen_alto_10, prmat_bajo_10_p, prmat_medio_10_p, prmat_alto_10_p, prlen_bajo_10_p, prlen_medio_10_p, prlen_alto_10_p, permed50len, permed33len, permed50mat, permedmat33, aprobacion5, aprobacion9, aprobacion11a, aprobacion11t
	  FROM  `res11_11` , res11
	  WHERE res11.cole_inst_nombre = res11_11.cole_inst_nombre;

	INSERT INTO `res11` (cole_inst_nombre, dane, aprobacion5, aprobacion9, aprobacion11a, aprobacion11t)
	 SELECT cole_inst_nombre, dane, aprobacion5, aprobacion9, aprobacion11a, aprobacion11t FROM `res111` 
	 WHERE NOT EXISTS (SELECT `cole_inst_nombre` FROM `res211` WHERE res111.cole_inst_nombre = res211.cole_inst_nombre);

END //

DELIMITER ;
