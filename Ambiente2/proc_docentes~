	 (select count(iii_6) from(select dane, iii_6 from clone_docentes) as a 
	 where a.dane = clone_docentes.dane and a.iii_6 = clone_docentes.iii_6 group by a.iii_6, a.dane);

	# egen f_iii_7_re_d=count(iii_7), by(iii_7 dane)
	update clone_docentes set f_iii_7_re_d =
	 (select count(iii_7) from(select dane, iii_7 from clone_docentes) as a 
	 where a.dane = clone_docentes.dane and a.iii_7 = clone_docentes.iii_7 group by a.iii_7, a.dane);

	# egen f_iii_8_re_d=count(iii_8), by(iii_8 dane)
