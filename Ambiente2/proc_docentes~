	 (select count(iii_4) from(select dane, iii_4 from clone_docentes) as a 
	 where a.dane = clone_docentes.dane and a.iii_4 = clone_docentes.iii_4 group by a.iii_4, a.dane);

	# egen f_iii_5_re_d=count(iii_5), by(iii_5 dane)
