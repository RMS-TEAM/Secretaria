	 f_vi_13_re_e, f_vi_14_re_e, f_vi_15_re_e, f_vi_16_re_e, f_vi_17_re_e, f_vi_18_re_e, f_vii_1_re_e,
	 pro_iii_1_re_e, pro_iii_2_re_e, pro_iii_3_re_e, pro_iii_4_re_e, pro_iii_5_re_e, pro_iii_6_re_e,
	 pro_iii_7_re_e, pro_iii_8_re_e, pro_iii_9_re_e, pro_iv_1_re_e, pro_iv_2_re_e, pro_iv_3_re_e,
	 pro_iv_4_re_e, pro_iv_5_re_e, pro_iv_6_re_e, pro_iv_7_re_e, pro_iv_8_re_e, pro_v_1_re_e, pro_v_2_re_e,
	 pro_v_3_re_e, pro_v_4_re_e, pro_v_5_re_e, pro_v_6_re_e, pro_v_7_re_e, pro_v_8_re_e, pro_v_9_re_e,
	 pro_v_10_re_e, pro_v_11_a_re_e, pro_v_11_b_re_e, pro_v_11_c_re_e, pro_v_11_d_re_e, pro_v_12_re_e,
	 pro_v_13_re_e, pro_v_14_re_e, pro_v_15_re_e, pro_vi_1_re_e, pro_vi_2_re_e, pro_vi_3_re_e, pro_vi_4_re_e,
	 pro_vi_5_re_e, pro_vi_6_re_e, pro_vi_7_re_e, pro_vi_8_re_e, pro_vi_9_re_e, pro_vi_10_re_e,
	 pro_vi_11_re_e, pro_vi_12_re_e, pro_vi_13_re_e, pro_vi_14_re_e, pro_vi_15_re_e, pro_vi_16_re_e,
	 pro_vi_17_re_e, pro_vi_18_re_e, pro_vii_1_re_e, III_e, IV_e, V_e, VI_e, VII_e FROM `temp_merge`
	 LEFT JOIN `padres1` ON `temp_merge`.`dane` = `padres1`.`dane`;

	INSERT INTO `merge` (dane, iii_1_re_p, iii_2_re_p, iii_3_re_p, iii_4_re_p, iii_5_re_p, iii_6_re_p,
	 iii_7_re_p, iii_8_re_p, iii_9_re_p, iv_1_re_p, iv_2_re_p, iv_3_re_p, iv_4_re_p, iv_5_re_p,
	 iv_6_re_p, iv_7_re_p, iv_8_re_p, iv_9_re_p, iv_10_re_p, iv_11_re_p, iv_12_re_p, v_1_re_p, v_2_re_p,
	 v_3_re_p, v_4_re_p, v_5_re_p, v_6_re_p, v_7_re_p, v_8_re_p, v_9_re_p, vi_1_re_p, vi_2_re_p, vi_3_re_p,
	 vi_4_re_p, vi_5_re_p, vi_6_re_p, vi_7_re_p, vi_8_re_p, vi_9_re_p, vi_10_re_p, vi_11_re_p, vii_1_re_p,
	 f_iii_1_re_p, f_iii_2_re_p, f_iii_3_re_p, f_iii_4_re_p, f_iii_5_re_p, f_iii_6_re_p, f_iii_7_re_p,
	 f_iii_8_re_p, f_iii_9_re_p, f_iv_1_re_p, f_iv_2_re_p, f_iv_3_re_p, f_iv_4_re_p, f_iv_5_re_p,
	 f_iv_6_re_p, f_iv_7_re_p, f_iv_8_re_p, f_iv_9_re_p, f_iv_10_re_p, f_iv_11_re_p, f_iv_12_re_p,
	 f_v_1_re_p, f_v_2_re_p, f_v_3_re_p, f_v_4_re_p, f_v_5_re_p, f_v_6_re_p, f_v_7_re_p, f_v_8_re_p,
	 f_v_9_re_p, f_vi_1_re_p, f_vi_2_re_p, f_vi_3_re_p, f_vi_4_re_p, f_vi_5_re_p, f_vi_6_re_p, f_vi_7_re_p,
	 f_vi_8_re_p, f_vi_9_re_p, f_vi_10_re_p, f_vi_11_re_p, f_vii_1_re_p, pro_iii_1_re_p, pro_iii_2_re_p,
	 pro_iii_3_re_p, pro_iii_4_re_p, pro_iii_5_re_p, pro_iii_6_re_p, pro_iii_7_re_p, pro_iii_8_re_p,
	 pro_iii_9_re_p, pro_iv_1_re_p, pro_iv_2_re_p, pro_iv_3_re_p, pro_iv_4_re_p, pro_iv_5_re_p,
	 pro_iv_6_re_p, pro_iv_7_re_p, pro_iv_8_re_p, pro_iv_9_re_p, pro_iv_10_re_p, pro_iv_11_re_p,
	 pro_iv_12_re_p, pro_v_1_re_p, pro_v_2_re_p, pro_v_3_re_p, pro_v_4_re_p, pro_v_5_re_p, pro_v_6_re_p,
	 pro_v_7_re_p, pro_v_8_re_p, pro_v_9_re_p, pro_vi_1_re_p, pro_vi_2_re_p, pro_vi_3_re_p, pro_vi_4_re_p,
	 pro_vi_5_re_p, pro_vi_6_re_p, pro_vi_7_re_p, pro_vi_8_re_p, pro_vi_9_re_p, pro_vi_10_re_p,
	 pro_vi_11_re_p, pro_vii_1_re_p, III_p, IV_p, V_p, VI_p, VII_p)
	 SELECT dane, iii_1_re_p, iii_2_re_p, iii_3_re_p, iii_4_re_p, iii_5_re_p, iii_6_re_p,
	 iii_7_re_p, iii_8_re_p, iii_9_re_p, iv_1_re_p, iv_2_re_p, iv_3_re_p, iv_4_re_p, iv_5_re_p,
	 iv_6_re_p, iv_7_re_p, iv_8_re_p, iv_9_re_p, iv_10_re_p, iv_11_re_p, iv_12_re_p, v_1_re_p, v_2_re_p,
	 v_3_re_p, v_4_re_p, v_5_re_p, v_6_re_p, v_7_re_p, v_8_re_p, v_9_re_p, vi_1_re_p, vi_2_re_p, vi_3_re_p,
	 vi_4_re_p, vi_5_re_p, vi_6_re_p, vi_7_re_p, vi_8_re_p, vi_9_re_p, vi_10_re_p, vi_11_re_p, vii_1_re_p,
	 f_iii_1_re_p, f_iii_2_re_p, f_iii_3_re_p, f_iii_4_re_p, f_iii_5_re_p, f_iii_6_re_p, f_iii_7_re_p,
	 f_iii_8_re_p, f_iii_9_re_p, f_iv_1_re_p, f_iv_2_re_p, f_iv_3_re_p, f_iv_4_re_p, f_iv_5_re_p,
	 f_iv_6_re_p, f_iv_7_re_p, f_iv_8_re_p, f_iv_9_re_p, f_iv_10_re_p, f_iv_11_re_p, f_iv_12_re_p,
	 f_v_1_re_p, f_v_2_re_p, f_v_3_re_p, f_v_4_re_p, f_v_5_re_p, f_v_6_re_p, f_v_7_re_p, f_v_8_re_p,
	 f_v_9_re_p, f_vi_1_re_p, f_vi_2_re_p, f_vi_3_re_p, f_vi_4_re_p, f_vi_5_re_p, f_vi_6_re_p, f_vi_7_re_p,
	 f_vi_8_re_p, f_vi_9_re_p, f_vi_10_re_p, f_vi_11_re_p, f_vii_1_re_p, pro_iii_1_re_p, pro_iii_2_re_p,
	 pro_iii_3_re_p, pro_iii_4_re_p, pro_iii_5_re_p, pro_iii_6_re_p, pro_iii_7_re_p, pro_iii_8_re_p,
	 pro_iii_9_re_p, pro_iv_1_re_p, pro_iv_2_re_p, pro_iv_3_re_p, pro_iv_4_re_p, pro_iv_5_re_p,
	 pro_iv_6_re_p, pro_iv_7_re_p, pro_iv_8_re_p, pro_iv_9_re_p, pro_iv_10_re_p, pro_iv_11_re_p,
	 pro_iv_12_re_p, pro_v_1_re_p, pro_v_2_re_p, pro_v_3_re_p, pro_v_4_re_p, pro_v_5_re_p, pro_v_6_re_p,
	 pro_v_7_re_p, pro_v_8_re_p, pro_v_9_re_p, pro_vi_1_re_p, pro_vi_2_re_p, pro_vi_3_re_p, pro_vi_4_re_p,
	 pro_vi_5_re_p, pro_vi_6_re_p, pro_vi_7_re_p, pro_vi_8_re_p, pro_vi_9_re_p, pro_vi_10_re_p,
	 pro_vi_11_re_p, pro_vii_1_re_p, III_p, IV_p, V_p, VI_p, VII_p FROM `padres1` 
	 WHERE NOT EXISTS (SELECT `dane` FROM `temp_merge` WHERE temp_merge.dane = padres1.dane);
	
	DROP TABLE `temp_merge`;


	#------------------------------------
	#   Aca va los ultimos egen rowmean |
	#------------------------------------

	UPDATE `merge` SET `III_total` = (`III_d` + `III_e` + `III_p`) / 3;
	UPDATE `merge` SET `IV_total` = (`IV_d` + `IV_e` + `IV_p`) / 3;
	UPDATE `merge` SET `V_total` = (`V_d` + `V_e` + `V_p`) / 3;
	UPDATE `merge` SET `VI_total` = (`VI_d` + `VI_e` + `VI_p`) / 3;
	UPDATE `merge` SET `VII_total` = (`VII_d` + `VII_e` + `VII_p`) / 3;

	UPDATE `merge` SET `Ambi_escol` = (`III_total` + `IV_total` + `V_total` + `VI_total`) / 4;
	
END //

